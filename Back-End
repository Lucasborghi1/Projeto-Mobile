import java.util.Scanner;

//Tela apolice

class apolice{
   public String codigo apolice;

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

System.out.println("0123456");
        String codigo apolice = in.nextLine();
        
if
    System.out.println("Data Vigencia: 24/05/2020");
in.nextLine();
System.out.println("Data Fim Vigencia: 24/05/2021");
in.nextLine();
System.out.println("Placa: ABCD 1234");
in.nextLine();
System.out.println("Modelo: Nivus Highline 200 TSI");
in.nextLine();
System.out.println("Cor: Branca");
in.nextLine();
System.out.println("Indenização: R$ 100.000,00");
in.nextLine(); 
    else
     System.out.println("Codigo da apolice incorreto" );
  } 
  }
