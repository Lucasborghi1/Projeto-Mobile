import java.util.Scanner;

//Tela dados do cliente

class tela dados{
   public String dados;

    public static void main(String[] args) {
    Scanner in = new Scanner(System.in);

System.out.println("cliente1");
        String dados = in.nextLine();
       
if
    System.out.println("cliente1");
in.nextLine();
System.out.println("cliente1@gmail.com");
in.nextLine();
System.out.println("000000000/01");
in.nextLine();
System.out.println("(11) 90101-0101");
in.nextLine();
    else
     System.out.println("Erro nos dados" );
  } 
  }
