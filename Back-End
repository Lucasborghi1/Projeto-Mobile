import javax.swing.JOptionPane;

MailMessage mail = new MailMessage();

//Tela recuperar senha II

class Recuperar senha II{

            mail.From = new MailAddress(ConfigurationManager.AppSettings["userName"]);
            mail.To.Add(emailPara);
            mail.Subject = assunto;
            mail.Body = corpo;

            // em caso de anexos
            // mail.Attachments.Add(new Attachment(@"C:\teste.txt"));

            // ENVIO DE EMAIL ASSÍNCRONO
            var smtp = new SmtpClient("smtp.gmail.com");
            smtp.EnableSsl = Convert.ToBoolean(ConfigurationManager.AppSettings["EnableSsl"]);
            smtp.Port = Convert.ToInt32(ConfigurationManager.AppSettings["Port"]);
            smtp.DeliveryMethod = SmtpDeliveryMethod.Network;
            smtp.UseDefaultCredentials = Convert.ToBoolean(ConfigurationManager.AppSettings["UseDefaultCredentials"]);
            smtp.Credentials = new NetworkCredential(ConfigurationManager.AppSettings["userName"], ConfigurationManager.AppSettings["appPassword"]);
            smtp.SendCompleted += (s, e) =>
            {
                smtp.Dispose();
            };
            smtp.SendAsync(mail, null);
